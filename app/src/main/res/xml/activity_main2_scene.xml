<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintVertical_bias="0.977"
            android:layout_height="1325dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.828"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="835dp"
            android:id="@+id/imageView4" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="230dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.964"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="129dp"
            android:id="@+id/imageView8"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_editor_absoluteX="655dp"
            motion:layout_editor_absoluteY="1050dp" />
        <Constraint
            android:layout_height="235dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="363dp"
            android:id="@+id/imageView6"
            motion:layout_editor_absoluteX="90dp"
            motion:layout_editor_absoluteY="1045dp" />
        <Constraint
            android:layout_height="200dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="820dp" />
        <Constraint
            android:layout_height="225dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="832dp"
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="@+id/imageView8"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="1055dp" />
    </ConstraintSet>
</MotionScene>